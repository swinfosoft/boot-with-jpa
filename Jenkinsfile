pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'code will be build here.'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing will be done here.'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deployment will be done here.'
      }
    }
  }
}